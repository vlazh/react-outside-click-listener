{
  "name": "react-outside-click-listener",
  "version": "0.1.0",
  "description": "Outside click listener for React",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/react-outside-click-listener"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json .npmignore README.md LICENSE ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/react-outside-click-listener' && copyfiles ./src/**/{*.css,*.woff2,*.woff,*.otf,*.eot,*.ttf,*.svg} --up 1 '/home/vladimir/dev/sources/tutoron/node_modules/react-outside-click-listener'",
    "patch-publish": "npm run build && npm version patch && npm run copy:configs && npm publish ./dist/ && git push origin master --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy:configs && npm publish ./dist/ && git push origin master --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy:configs && npm publish ./dist/ && git push origin master --follow-tags"
  },
  "devDependencies": {
    "@types/react": "16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@vzh/configs": "^3.54.1",
    "copyfiles": "^2.2.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  }
}
